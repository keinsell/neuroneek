{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "format": {},
    "lint": {
      "outputs": []
    },
    "test": {
      "inputs": [
        "src/**/*.test.ts"
      ],
      "dependsOn": [
        "^build",
        "^db:generate"
      ],
      "cache": false
    },
    "prebuild": {
      "outputs": [
        "dist/**",
        ".next/**",
        "public/dist/**",
        "node_modules/@prisma/client"
      ],
      "cache": true
    },
    "build": {
      "dependsOn": [
        "^prebuild",
        "^build",
        "^db:generate"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "public/dist/**"
      ],
      "cache": true
    },
    "start": {
      "dependsOn": [
        "^build"
      ],
      "cache": false
    },
    "dev": {
      "cache": false,
      "dependsOn": [
        "^build"
      ]
    },
    "db:migrate:deploy": {
      "outputs": [],
      "cache": false
    },
    "db:push": {
      "outputs": [],
      "cache": false
    },
    "db:seed": {
      "outputs": [],
      "cache": false
    },
    "db:studio": {
      "outputs": [],
      "cache": false
    },
    "db:generate": {
      "cache": false
    }
  },
  "remoteCache": {
    "signature": true
  }
}
