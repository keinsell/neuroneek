  "scripts": {
    "build:nest": "nest build",
    "build": "pnpm run build:tsc",
    "build:tsc": "tsc -p tsconfig.json && pnpm copy:markdown && pnpm copy:prisma && pnpm build:copy:docs",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "lint:fta": "npx fta-cli --config-path .fta.json src",
    "copy:markdown": "copyfiles -e \"**/node_modules/**\" -u 1 \"src/**/*.md\" dist/src",
    "copy:prisma": "copyfiles -u 1 \"vendor/prisma/**\" dist/src",
    "build:copy:docs": "copyfiles -u 1 \"src/common/modules/documentation/swagger/public/**\" dist/src",
    "dev": "pnpm run dev:tsc",
    "dev:keploy": "sudo -E keploy record -c \"pnpm dev\"",
    "test:keploy": "sudo -E keploy test",
    "dev:tsc": "nodemon",
    "dev:nest": "nest start --watch",
    "dev:tsx": "tsx src/main.ts --clear",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "stepci run e2e/stepci/account.workflow.yml",
    "db:generate": "prisma generate",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:push:force": "prisma db push --accept-data-loss",
    "db:reset": "prisma db push --force-reset --accept-data-loss",
    "db:pull": "prisma db pull",
    "db:studio": "prisma studio",
    "postinstall": "pnpm run db:generate",
    "prepare": "ts-patch install && typia patch -s && pnpm run db:generate",
    "compodoc": "compodoc -p tsconfig.compodoc.json --output vendor/compodoc --theme laravel",
    "sentry:sm:inject": "sentry-cli sourcemaps inject dist --org sentry",
    "sentry:sm:upload": "sentry-cli sourcemaps upload dist --org sentry --project orca"
  },
