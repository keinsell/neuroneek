  "info": {
    "title": "${PROJECT}",
    "description": "Methylophenidate is a boilerplate for Nest.js applications with batteries included.",
    "version": "1.0",
    "contact": {}
  },
  "tags": [
    {
      "name": "api",
      "description": ""
    }
  ],
  "servers": [],
  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "name": "Bearer Token",
        "type": "http",
        "description": "JWT Authorization header using the Bearer scheme. Example: \"Authorization: Bearer <token>\""
      }
    },
    "schemas": {
      "InitiateSingleSignOn": {
        "type": "object",
        "properties": {
          "provider": {
            "type": "string",
            "enum": [
              "github"
            ],
            "example": "github"
          },
          "redirectUrl": {
            "type": "string",
            "example": "http://localhost:1337/sso/github"
          }
        },
        "required": [
          "provider",
          "redirectUrl"
        ]
      },
      "InitiateCertificateBasedAuthentication": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "description": "The account's username",
            "example": "Raul_Marks",
            "examples": [
              "Floy.Daniel",
              "Providenci.McCullough52",
              "Cydney_Walker39"
            ]
          }
        },
        "required": [
          "username"
        ]
      },
      "AddItemToCart": {
        "type": "object",
        "properties": {
          "productId": {
            "type": "string",
            "example": "gpu.nvidia.rtx.4090"
          },
          "quantity": {
            "type": "number",
            "example": 1
          }
        },
        "required": [
          "productId",
          "quantity"
        ]
      },
      "CheckoutPaymentMethods": {
        "type": "object",
        "properties": {
          "paymentMethods": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "example": [
              "stripe"
            ]
          }
        },
        "required": [
          "paymentMethods"
        ]
      },
      "CheckoutStripePaymentMethod": {
        "type": "object",
        "properties": {
          "customerId": {
            "type": "string",
            "example": "cus_J3z3v2eZvKYlo2C5z3v2eZvKYlo"
          },
          "paymentMethodId": {
            "type": "string",
            "example": "pm_1J3z3v2eZvKYlo2C5z3v2eZvKYlo"
          }
        }
      },
      "CheckoutPaymentMethod": {
        "type": "object",
        "properties": {
          "STRIPE": {
            "$ref": "#/components/schemas/CheckoutStripePaymentMethod"
          }
        }
      },
      "AddCheckoutItem": {
        "type": "object",
        "properties": {
          "productId": {
            "type": "string",
            "description": "The product ID",
            "example": "gpu.nvidia.rtx.4090"
          },
          "quantity": {
            "type": "number",
            "example": 1
          }
        },
        "required": [
          "productId",
          "quantity"
        ]
      },
      "InitializeCheckout": {
        "type": "object",
        "properties": {
          "paymentMethod": {
            "$ref": "#/components/schemas/CheckoutPaymentMethod"
          },
          "items": {
            "type": "array",
