{
  "output_limit": 250,
  "score_cap": 90,
  "exclude_directories": [
    "__fixtures__",
    "vendor",
    "build",
    "dist",
    "coverage",
    "node_modules",
    "public"
  ],
  "exclude_filenames": [
    "*.test.{ts,tsx}",
    "*.spec.{ts,tsx}"
  ],
  "extensions": [
    ".ts"
  ],
  "include_comments": false,
  "exclude_under": 30
}