{
	"name": "database",
	"main": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist/**"
	],
	"scripts": {
		"prebuild": "yarn db:generate",
		"build": "tsup-node",
		"db:generate": "prisma generate",
		"db:push": "prisma db push --skip-generate",
		"predev": "npm run generate",
		"dev": "tsup-node --watch"
	},
	"dependencies": {
		"@prisma/client": "latest"
	},
	"devDependencies": {
		"prisma": "latest",
		"tsup": "6.2.2",
		"typescript": "4.7.4"
	}
}
